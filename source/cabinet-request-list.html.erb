---
layout: "cabinet"
title: "Список заявок"
---
<div class="container">
	<div class="row">
		<div class="col-md-3">
			<%= partial "cabinet-menu" %>
		</div>
		<div class="col-md-9">
			<div class="title clearfix">			
				<h3>Список заявок</h3>			
				<a href="cabinet-request-create.html" class="btn btn-success btn-lg pull-right"><i class="icon-plus"></i> Добавить заявку</a>
			</div>
			<div class="table-responsive">
				<table class="table table-hover table-request table-bordered table-striped">
					<thead>
						<tr>
							<th colspan="3">
								<div class="input-group col-md-12">
									<span class="input-group-btn">
										<button class="btn btn-default" type="button"><i class="icon-search"></i></button>
									</span>
									<input type="text" class="form-control">
								</div>
							</th>
							<th width="300">
								<select class="form-control">
									<option>По дате</option>
									<option>По номеру</option>
								</select>
							</th>
						</tr>
						<tr>
							<th>№</th>
							<th>Название</th>
							<th>Дата создания</th>
							<th>Действия</th>
						</tr>
					</thead>
					<tbody>
						<tr rel="href" data-href="cabinet-request-modify.html">
							<td>1</td>
							<td>1156</td>
							<td>13.12.2013</td>
							<td>
								<div class="btn-group">
									<button type="button" class="btn btn-primary print">
										<i class="icon-print icon-large" data-toggle="tooltip" title="" data-original-title="Распечатать"></i>
									</button>
									<button type="button" class="btn btn-success bill">
										<i class="icon-usd icon-large" data-toggle="tooltip" title="" data-original-title="Оплатить счет"></i>
									</button>
									
									<a href="cabinet-request-modify.html" type="button" class="btn btn-warning modify">
										<i class="icon-edit icon-large" data-toggle="tooltip" title="" data-original-title="Изменить"></i>
									</a>
									<button type="button" class="btn btn-danger remove">
										<i class="icon-remove icon-large" data-toggle="tooltip" title="" data-original-title="Удалить"></i>
									</button>
								</div>
							</td>
						</tr>
						<tr rel="href" data-href="cabinet-request-modify.html">
							<td>2</td>
							<td>1157</td>
							<td>13.12.2013</td>
							<td>
								<div class="btn-group">
									<button type="button" class="btn btn-primary print">
										<i class="icon-print icon-large" data-toggle="tooltip" title="" data-original-title="Распечатать"></i>
